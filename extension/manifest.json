{
  "manifest_version": 2,
  "name": "Source Quicklinks",
  "version": "1.3.1",
  "description": "Allows quick jumping between WebKit, Chrome, V8, etc. source links",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": ["js/closure-lite.js", "js/link.js", "js/background.js"]
  },
  "page_action": {
    "default_icon": "icon19.png",
    "default_title": "Source Quicklinks",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["*://code.google.com/searchframe"],
      "js": ["js/code-search-frame.user.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "omnibox": {
    "keyword" : "sql"
  }
}
